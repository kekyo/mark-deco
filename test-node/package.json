{
  "name": "mark-deco-node-tests",
  "version": "0.0.1",
  "description": "Node.js application for mark-deco library",
  "keywords": [
    "markdown",
    "processor",
    "node",
    "console"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "md-processor": "dist/index.js"
  },
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "start": "node dist/index.js",
    "test": "npm run build && vitest run",
    "pack": "echo \"mark-deco-node-tests does not have packing rule.\""
  },
  "dependencies": {
    "commander": ">=12.1.0",
    "mark-deco": "*"
  },
  "devDependencies": {
    "@types/node": ">=20.0.0",
    "prettier-max": ">=1.12.0",
    "typescript": ">=5.0.0",
    "vite": ">=6.3.5",
    "vitest": ">=1.0.0"
  }
}
